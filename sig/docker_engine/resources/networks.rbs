module DockerEngine
  module Resources
    class Networks
      def create: (
        name: String,
        ?attachable: bool,
        ?config_from: DockerEngine::NetworkCreateParams::ConfigFrom,
        ?config_only: bool,
        ?driver: String,
        ?enable_i_pv4: bool,
        ?enable_i_pv6: bool,
        ?ingress: bool,
        ?internal: bool,
        ?ipam: DockerEngine::NetworkCreateParams::Ipam,
        ?labels: ::Hash[Symbol, String],
        ?options: ::Hash[Symbol, String],
        ?scope: String,
        ?request_options: DockerEngine::request_opts
      ) -> DockerEngine::CreateResponse

      def list: (
        ?filters: String,
        ?request_options: DockerEngine::request_opts
      ) -> DockerEngine::Models::network_list_response

      def delete: (
        String id,
        ?request_options: DockerEngine::request_opts
      ) -> nil

      def connect: (
        String id,
        container: String,
        ?endpoint_config: DockerEngine::ConnectRequest::EndpointConfig,
        ?request_options: DockerEngine::request_opts
      ) -> nil

      def disconnect: (
        String id,
        container: String,
        ?force: bool,
        ?request_options: DockerEngine::request_opts
      ) -> nil

      def inspect_: (
        String id,
        ?scope: String,
        ?verbose: bool,
        ?request_options: DockerEngine::request_opts
      ) -> DockerEngine::Models::NetworkInspectResponse

      def prune: (
        ?filters: String,
        ?request_options: DockerEngine::request_opts
      ) -> DockerEngine::Models::NetworkPruneResponse

      def initialize: (client: DockerEngine::Client) -> void
    end
  end
end
