module DockerEngine
  module Models
    type wait_response =
      { status_code: Integer, error: DockerEngine::WaitResponse::Error }

    class WaitResponse < DockerEngine::Internal::Type::BaseModel
      attr_accessor status_code: Integer

      attr_reader error: DockerEngine::WaitResponse::Error?

      def error=: (
        DockerEngine::WaitResponse::Error
      ) -> DockerEngine::WaitResponse::Error

      def initialize: (
        status_code: Integer,
        ?error: DockerEngine::WaitResponse::Error
      ) -> void

      def to_hash: -> {
        status_code: Integer,
        error: DockerEngine::WaitResponse::Error
      }

      type error = { message: String }

      class Error < DockerEngine::Internal::Type::BaseModel
        attr_reader message: String?

        def message=: (String) -> String

        def initialize: (?message: String) -> void

        def to_hash: -> { message: String }
      end
    end
  end
end
