module DockerEngineAPI
  module Resources
    class Networks
      def create: (
        name: String,
        ?attachable: bool,
        ?config_from: DockerEngineAPI::NetworkCreateParams::ConfigFrom,
        ?config_only: bool,
        ?driver: String,
        ?enable_i_pv4: bool,
        ?enable_i_pv6: bool,
        ?ingress: bool,
        ?internal: bool,
        ?ipam: DockerEngineAPI::NetworkCreateParams::Ipam,
        ?labels: ::Hash[Symbol, String],
        ?options: ::Hash[Symbol, String],
        ?scope: String,
        ?request_options: DockerEngineAPI::request_opts
