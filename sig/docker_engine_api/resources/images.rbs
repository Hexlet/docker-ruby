module DockerEngineAPI
  module Resources
    class Images
      def list: (
        ?all: bool,
        ?digests: bool,
        ?filters: String,
        ?manifests: bool,
        ?shared_size: bool,
        ?request_options: DockerEngineAPI::request_opts
      ) -> DockerEngineAPI::Models::image_list_response

      def delete: (
        String name,
        ?force: bool,
        ?noprune: bool,
        ?platforms: ::Array[String],
        ?request_options: DockerEngineAPI::request_opts
      ) -> DockerEngineAPI::Models::image_delete_response

      def build: (
        body: DockerEngineAPI::Internal::file_input,
        ?buildargs: String,
        ?cachefrom: String,
        ?cpuperiod: Integer,
        ?cpuquota: Integer,
        ?cpusetcpus: String,
        ?cpushares: Integer,
        ?dockerfile: String,
        ?extrahosts: String,
        ?forcerm: bool,
        ?labels: String,
        ?memory: Integer,
        ?memswap: Integer,
        ?networkmode: String,
        ?nocache: bool,
        ?outputs: String,
        ?platform: String,
        ?pull: String,
        ?q: bool,
        ?remote: String,
        ?rm: bool,
        ?shmsize: Integer,
        ?squash: bool,
        ?t: String,
        ?target: String,
        ?version: DockerEngineAPI::Models::ImageBuildParams::version,
        ?x_registry_config: String,
        ?request_options: DockerEngineAPI::request_opts
      ) -> nil

      def build_prune: (
        ?all: bool,
        ?filters: String,
        ?max_used_space: Integer,
        ?min_free_space: Integer,
        ?reserved_space: Integer,
        ?request_options: DockerEngineAPI::request_opts
      ) -> DockerEngineAPI::Models::ImageBuildPruneResponse

      def commit: (
        ?author: String,
        ?changes: String,
        ?comment: String,
        ?container: String,
        ?pause: bool,
        ?repo: String,
        ?tag: String,
        ?args_escaped: bool?,
        ?attach_stderr: bool,
        ?attach_stdin: bool,
        ?attach_stdout: bool,
        ?cmd: ::Array[String],
        ?domainname: String,
        ?entrypoint: ::Array[String],
        ?env: ::Array[String],
        ?exposed_ports: ::Hash[Symbol, top]?,
        ?healthcheck: DockerEngineAPI::Config::Healthcheck,
        ?hostname: String,
        ?image: String,
        ?labels: ::Hash[Symbol, String],
        ?network_disabled: bool?,
        ?on_build: ::Array[String]?,
        ?open_stdin: bool,
        ?shell: ::Array[String]?,
        ?stdin_once: bool,
        ?stop_signal: String?,
        ?stop_timeout: Integer?,
        ?tty: bool,
        ?user: String,
        ?volumes: ::Hash[Symbol, top],
        ?working_dir: String,
        ?request_options: DockerEngineAPI::request_opts
      ) -> DockerEngineAPI::Models::ImageCommitResponse

      def get: (
        String name,
        ?platform: ::Array[String],
        ?request_options: DockerEngineAPI::request_opts
      ) -> StringIO

      def get_all: (
        ?names: ::Array[String],
        ?platform: ::Array[String],
        ?request_options: DockerEngineAPI::request_opts
      ) -> StringIO

      def history: (
        String name,
        ?platform: String,
        ?request_options: DockerEngineAPI::request_opts
      ) -> DockerEngineAPI::Models::image_history_response

      def inspect_: (
        String name,
        ?manifests: bool,
        ?request_options: DockerEngineAPI::request_opts
      ) -> DockerEngineAPI::Image

      def load_: (
        body: DockerEngineAPI::Internal::file_input,
        ?platform: ::Array[String],
        ?quiet: bool,
        ?request_options: DockerEngineAPI::request_opts
      ) -> nil

      def prune: (
        ?filters: String,
        ?request_options: DockerEngineAPI::request_opts
      ) -> DockerEngineAPI::Models::ImagePruneResponse

      def pull: (
        ?changes: ::Array[String],
        ?from_image: String,
        ?from_src: String,
        ?message: String,
        ?platform: String,
        ?repo: String,
        ?tag: String,
        ?body: String,
        ?x_registry_auth: String,
        ?request_options: DockerEngineAPI::request_opts
      ) -> nil

      def push: (
        String name,
        x_registry_auth: String,
        ?platform: String,
        ?tag: String,
        ?request_options: DockerEngineAPI::request_opts
      ) -> nil

      def search: (
        term: String,
        ?filters: String,
        ?limit: Integer,
        ?request_options: DockerEngineAPI::request_opts
      ) -> DockerEngineAPI::Models::image_search_response

      def tag: (
        String name,
        ?repo: String,
        ?tag: String,
        ?request_options: DockerEngineAPI::request_opts
      ) -> nil

      def initialize: (client: DockerEngineAPI::Client) -> void
    end
  end
end
